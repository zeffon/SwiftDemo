# Top level makefile for Perfect

all:
	cd Authenticator && $(MAKE)
	cd "Tap Tracker" && $(MAKE)
	cd "Upload Enumerator" && $(MAKE)
	cd "URL Routing" && $(MAKE)

clean:
	cd Authenticator && $(MAKE) clean
	cd "Tap Tracker" && $(MAKE) clean
	cd "Upload Enumerator" && $(MAKE) clean
	cd "URL Routing" && $(MAKE) clean
